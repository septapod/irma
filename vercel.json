{
  "version": 2,
  "name": "irma",
  "builds": [
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "rewrites": [
    { "source": "/(styles.css|script.js)", "destination": "/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "env": {
    "GPT_API_KEY": "@gpt_api_key"
  }
}
